{
  "routes": [
    {
      "route": "/form/*",
      "serve": "/index.html"
    },
    {
      "route": "/admin/*",
      "serve": "/index.html"
    },
    {
      "route": "/login",
      "serve": "/index.html"
    },
    {
      "route": "/forgot-password",
      "serve": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.ico",
      "/*.css",
      "/*.js"
    ]
  }
}
