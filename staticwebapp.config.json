{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.ico",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.svg",
      "/*.gif",
      "/*.woff",
      "/*.woff2",
      "/*.ttf",
      "/*.eot"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css"
  }
}
